{
  "name": "n8n-nodes-inflow-inventory",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for inFlow Inventory providing 12 resources and 60+ operations for complete inventory management, sales order processing, and purchase order handling.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-inflow-inventory.git"
  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-inflow-inventory",
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "inflow",
    "inventory",
    "inventory-management",
    "sales-orders",
    "purchase-orders",
    "stock-management",
    "warehouse",
    "small-business",
    "ecommerce"
  ],
  "main": "dist/nodes/InFlowInventory/InFlowInventory.node.js",
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/InFlowInventoryApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/InFlowInventory/InFlowInventory.node.js",
      "dist/nodes/InFlowInventory/InFlowInventoryTrigger.node.js"
    ]
  },
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build",
    "clean": "rimraf dist"
  },
  "files": [
    "dist",
    "LICENSE",
    "COMMERCIAL_LICENSE.md",
    "LICENSING_FAQ.md"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-workflow": "^1.35.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "n8n-workflow": ">=1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
